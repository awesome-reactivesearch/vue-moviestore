{
  "version": 2,
  "routes": [
    
    {
      "src": "/api/checkout-api.js",
      "use": "@vercel/node"
    },
    {
        "src": "/sw.js",
        "continue": true,
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate",
          "Service-Worker-Allowed": "/"
        }
    }
  ],
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["api/**"]
      }
    },
    {
        "src": "api/**/*.js",
        "use": "@vercel/node"
    }
  ]
}